{
  "name": "@design-system/react-components-box",
  "packageManager": "yarn@4.1.1",
  "version": "0.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "build": "yarn build:js & yarn build:type",
