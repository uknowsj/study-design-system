  },
  "devDependencies": {
    "@design-system/scripts": "workspace:^",
    "@design-system/themes": "workspace:^",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vanilla-extract/css": "^1.15.2",
    "@vanilla-extract/esbuild-plugin": "^2.3.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@design-system/themes": "workspace:^",
    "react": "*"
  }
}
